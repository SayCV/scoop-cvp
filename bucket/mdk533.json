        "$escdir = $EVAL_INST_PATH -replace \"\\\\\",\"\\\\\"",
        "(gc \"$bucketdir\\..\\scripts\\mdk533\\install-context.reg.templ\") | % {$_ -replace \"%EVAL_INST_PATH%\",\"$escdir\"} | out-file -enc oem \"$dir\\install-context.reg\"",
