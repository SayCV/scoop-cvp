{
    "version": "1.4.6",
    "description": "Deno Version Manager - Easy way to manage multiple active deno versions.",
    "homepage": "https://github.com/justjavac/dvm",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/justjavac/dvm/releases/download/v1.4.6/dvm-x86_64-pc-windows-msvc.zip",
            "hash": "267de2fcc5ca0a502dcf5e752720edc74cf6a108fc721b7dad7328c55db17ee8"
        }
    },
    "post_install": [
        "",
        "[Environment]::SetEnvironmentVariable('DVM_DIR', \"$dir/.dvm\", 'Process')",
        "[Environment]::SetEnvironmentVariable('DENO_INSTALL', \"$dir/.dvm/deno\", 'Process')",
        "Invoke-ExternalCommand -Path \"$dir/dvm.exe\" -Args 'install 1.16.1' | Out-Null",
        "Invoke-ExternalCommand -Path \"$dir/dvm.exe\" -Args 'use     1.16.1' | Out-Null",
        "Write-Output \"Done.\"",
        "",
        ""
    ],
    "bin": "dvm.exe",
    "env_add_path": [
        ".dvm/deno/bin"
    ],
    "env_set": {
        "DVM_DIR": "$dir/.dvm",
        "DENO_INSTALL": "$dir/.dvm/deno",
        "DENO_DIR": "$dir/.dvm/deno/appdata"
    },
    "persist": [
        ".dvm"
    ],
    "checkver": {
        "github": "https://github.com/justjavac/dvm"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/justjavac/dvm/releases/download/v$version/dvm-x86_64-pc-windows-msvc.zip"
            }
        }
    }
}
