{
    "version": "1.4.24",
    "description": "Powerful Bash-style command line editing for cmd.exe",
    "homepage": "https://chrisant996.github.io/clink/",
    "license": "GPL-3.0-only",
    "notes": [
        "Run 'clink inject' to start clink on the current cmd",
        "Run 'clink autorun install' to auto start clink"
    ],
    "depends": "",
    "url": "https://ghproxy.com/https://github.com/chrisant996/clink/releases/download/v1.4.24/clink.1.4.24.688975.zip",
    "hash": "e2c790a4e8396ede277d8c6acd6ff4169c4bc185b5737890875acc26f95cd104",
    "pre_install": [
        "",
        "Write-Host \":: Create script for start clink...\" -ForegroundColor DarkCyan",
        "$ofile = \"$dir/cmd_by_clink_portable.cmd\"",
        "$cont = \"@call doskey.exe /macrofile=$dir/profile/user_aliases.cmd\n@cmd /s /k clink inject --quiet --profile $dir/profile\"",
        "Set-Content \"$ofile\" \"$cont\" -Encoding ASCII",
        "",
        "Write-Host \":: Allow called by 'cmd_by_clink'\" -ForegroundColor DarkCyan",
        "",
        ""
    ],
    "bin": [
        [
            "clink.bat",
            "clink",
            ""
        ],
        "cmd_by_clink_portable.cmd"
    ],
    "persist": "profile",
    "checkver": {
        "url": "https://api.github.com/repos/chrisant996/clink/releases",
        "regex": "/v(?<tag>[\\d.]+)/clink\\.(?<version>[\\d.]+)\\.(?<commit>[\\w.]+)\\.zip"
    },
    "autoupdate": {
        "url": "https://ghproxy.com/https://github.com/chrisant996/clink/releases/download/v$matchTag/clink.$matchVersion.$matchCommit.zip"
    }
}
