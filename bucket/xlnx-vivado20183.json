    "pre_install": [
        "",
        "$null, $bucket = find_manifest($app)",
        "$bucketdir = Find-BucketDirectory($bucket)",
        "",
        "'data', 'home', 'scripts' | ForEach-Object { ensure \"$dir\\$_\" | Out-Null }",
        "",
        "Write-Host \"S2. Creating scripts...\" -ForegroundColor DarkCyan",
