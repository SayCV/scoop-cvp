        "$ofile = \"$dir\\scripts\\xlnx_vivado.cmd\"",
        "$cont = (Get-Content \"$ifile\") -replace '@SCRIPT_NAME@', \"xlnx_vivado\"",
        "$cont = $cont -replace '@SCRIPT_RUNNER_RELDIR@', \"Vivado/2018.3/doc/images\"",
